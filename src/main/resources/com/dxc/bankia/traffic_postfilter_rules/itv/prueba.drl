package com.dxc.bankia.traffic_postfilter_rules.itv;

import com.dxc.bankia.model.Vehicle;
import com.dxc.bankia.model.functions.DateUtils;

rule "itv_period_5"
	dialect "mvel"
	when
		$c : Vehicle( age >= 5 , age < 100 , country == Country.ES , category == Vehicle.Category.BIKE , nextItvDate == null )
	then		
		
		modify ($c){ $c.setNextItvDate(null)};
end
