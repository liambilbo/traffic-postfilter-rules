package com.dxc.bankia.traffic_postfilter_rules_kjar.itv;

import java.lang.Number;
import com.dxc.bankia.model.Event;
import com.dxc.bankia.model.Vehicle;
import java.util.Date;
import com.dxc.bankia.model.functions.DateUtils;

rule "event-itv-compliance"
	dialect "mvel"
	when
		$e : Event( type == Event.Type.REQUEST_CAR_ITV_COMPLIANCE , vehicle != null , vehicle.nextItvDate before ( today() ) )
	then
end
